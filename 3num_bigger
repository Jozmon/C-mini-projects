#include <stdio.h>

int main() {
	
	
	int a;
	int b;
	int c;
	
	printf ("Enter number a:");
	scanf ("%d",&a);
	
	printf ("Enter number b:");
	scanf ("%d",&b);
	
	printf ("Enter number c:");
	scanf ("%d",&c);	
	
	if (a>b){
		printf ("Number a is bigger");
	}else if (b>c){
		printf ("Number b is bigger");
	}else if (c>a){
		printf ("Number c is bigger");
	}else if (b>c){
		printf ("Number b is bigger");	
	}
	
	
	
	
	
	return 0;

}
